{
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "printWidth": 90,
  "tabWidth": 2,
  "semi": true,
  "quoteProps": "preserve",
  "endOfLine": "auto",
  "singleQuote": true,
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "singleQuote": false
      }
    }
  ]
}
